import java.util.*;
public class medium1{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter a word");
    String s = sc.nextLine();
    int n = s.length();
    int a[] = new int[n];
    
    for(int i=0; i<=n; i++){
      for(int j=0; j<i; j++){
        char ch = (char)j;
        System.out.print(ch);
      }
      System.out.println();
    }
  }
}
