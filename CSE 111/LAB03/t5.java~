import java.util.*;
public class t5{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter a word");
    String s = sc.nextLine();
    for(int i=1; i<3; i++){
      if(i==2){
        System.out.println(s+"==THE END==");
      }
      else{
        System.out.println(s);
      }
    }
  }
}
