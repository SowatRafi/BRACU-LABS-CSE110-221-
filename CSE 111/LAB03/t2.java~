import java.util.*;
public class t2{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter a word");
    String s = sc.nextLine();
    int length = s.length();
    for(int i=0; i<length; i++){
      char aChar = s.charAt(i);
      System.out.println(aChar);
    }
  }
}