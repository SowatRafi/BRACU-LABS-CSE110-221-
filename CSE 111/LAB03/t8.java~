import java.util.*;
public class t8{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter a word");
    String s = sc.nextLine();
    int l = s.length();
    
    for(int i=0; i<l; i++){
      char a = s.charAt(i);
      int j = s.codePointAt(i);
      
      for(int k=i+1; k<=i; k++){
        char b = s.charAt(k);
        int o = s.codePointAt(k);
//        System.out.println(o);
//        int r = .compareTo();
        
        
        if(j>o){
          System.out.println( Character.toString((char)j));
        }
        else{
          System.out.println( Character.toString((char)i));
        }
      }
    }
  }
}
